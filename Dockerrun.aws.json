{
    "AWSEBDockerrunVersion": 2,
	"family": "neservice",
    "volumes": [],
	"networkMode": "bridge",
    "containerDefinitions": [
        {
            "essential": true,
            "name": "backend",
			"image": "public.ecr.aws/neservice/backend:latest",
			"memory": 512,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        },
        {
            "essential": true,
            "name": "client",
			"image": "public.ecr.aws/neservice/client:latest",
			"memory": 124,
            "portMappings": [
                {
                    "containerPort": 4962,
                    "hostPort": 4962
                }
            ]
        },
        {
            "essential": true,
            "name": "nginx",
			"image": "public.ecr.aws/neservice/nginx:latest",
			"memory": 124,
			"links": ["client","backend"],
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]    
}
